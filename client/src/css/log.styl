@import 'nib'
@import 'variables.styl'

#log
    height 150px
    margin 15px 0
    padding 5px
    border-top 1px solid $alt
    overflow hidden
    overflow-y scroll
    text-align left
    span + span
        margin-left 10px

    .info
        color $log-info
    .warning
        color $log-warning
    .error
        color $log-error
        